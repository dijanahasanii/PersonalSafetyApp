{
  "expo": {
    "name": "Your App Name",
    "slug": "your-app-slug",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.yourname.yourapp"
    },
    "android": {
      "package": "com.yourname.yourapp"
    },
    "plugins": [
      "expo-font"
    ]
  }
}
